<!-- Hide & Show Script -->
 
<script>
$(document).ready(function(){
$(".tags, .links-content").hide();
$(".tags-title, .links-title").click(function(){
$(this).next(".tags, .links-content").slideToggle('fast');
});
});
</script>

<!-- Scroll To Top Script -->

<script type="text/javascript">
$(function() {      
$(window).scroll(function() {
if($(this).scrollTop() != 0) {
$('#top').fadeIn();         
} 
else {
$('#top').fadeOut();      
}
});
$('#top').click(function() {
$('body,html').animate({scrollTop:0},600);
});
});
</script>

