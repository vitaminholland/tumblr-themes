<script>
function opensite() {
document.getElementById("site").style.width = "100%";
document.getElementById("landing").style.marginLeft = "100%";
}
function closesite() {
document.getElementById("site").style.width = "0";
document.getElementById("landing").style.marginLeft = "0";
}
</script>

