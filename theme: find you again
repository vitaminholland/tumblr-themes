$(this).next(".tags").slideToggle('fast');
});
});
</script>

<!-- Scroll To Top Script -->

<script type="text/javascript">
$(function() {      
$(window).scroll(function() {
if($(this).scrollTop() != 0) {
$('#top').fadeIn();         
} 
else {
$('#top').fadeOut();      
}
});
$('#top').click(function() {
$('body,html').animate({scrollTop:0},600);
});
});
</script>

