<!-- Off Canvas Script -->

<script>
function openNav() {
document.getElementById("panel").style.width = "250px";
document.getElementById("canvas").style.marginLeft = "250px";
}
function closeNav() {
document.getElementById("panel").style.width = "0";
document.getElementById("canvas").style.marginLeft = "0";
}
</script>

